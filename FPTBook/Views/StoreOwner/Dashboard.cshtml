@{
    ViewData["Title"] = "ManageBook";
    Layout = "~/Views/Shared/_LayoutStoreOwner.cshtml";
}

<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FPTBook</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/FPTBook.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,0,0" />
    <link rel="stylesheet" href="~/css/swiper-bundle.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/remixicon/3.6.0/remixicon.css" />
    <script src="https://kit.fontawesome.com/63149990bf.js" crossorigin="anonymous"></script>
    <link rel="shortcut icon"
          href="assets/img/favicon.png"
          type="image/x-icon" />
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/remixicon/3.5.0/remixicon.css" />
    <link rel="stylesheet" href="~/css/swiper-bundle.min.css" />

    <link rel="stylesheet" href="~/css/site.css" />

    <title>Responsive book website</title>
</head>
<body>
    <div class="manage_book-container">
        <siline class="siline-main">

            <div class="navbar-item-A1">

                <ul class="navbar-nav">
                    <li class="nav-item-main">
                        <p class="">ANALYIS</p>
                    </li>

                    <li class="nav-item-abc1">
                        <a class=" active-main" asp-area="" asp-controller="StoreOwner" asp-action="Dashboard">
                            <span class="nav-link-text">
                                <i class="fa-solid fa-magnifying-glass-chart"></i>
                                <linkk> Table Analyis</linkk>
                            </span>
                        </a>
                    </li>

                    <li class="nav-item-main">
                        <p class="">MANAGE</p>
                    </li>

                    <li class="nav-item-abc1">
                        <a class="active-main" asp-area="" asp-controller="StoreOwner" asp-action="ManageBook">
                            <span class="nav-link-text ">
                                <i class="fa-solid fa-user"></i>
                                <linkk> Book</linkk>
                            </span>
                        </a>
                    </li>


                    <li class="nav-item-abc1">
                        <a class=" active-main" asp-area="" asp-controller="StoreOwner" asp-action="Record">
                            <span class="nav-link-text ">
                                <i class="fa-solid fa-cart-shopping"></i>
                                <linkk> Record</linkk>
                            </span>
                        </a>
                    </li>
                </ul>
            </div>
        </siline>
    </div>


    <script src="~/js/swiper-bundle.min.js"></script>
    <script src="~/js/scrollreveal.min.js"></script>
    <script src="~/js/main.js"></script>
</body>
</html>
